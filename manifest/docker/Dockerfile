FROM loads/alpine:3.8

###############################################################################
#                                INSTALLATION
###############################################################################

ENV WORKDIR  /app
ADD ./temp/linux_amd64/gf-demo-user $WORKDIR/gf-demo-user
RUN chmod +x $WORKDIR/gf-demo-user

###############################################################################
#                                   START
###############################################################################
WORKDIR $WORKDIR
CMD ./main
